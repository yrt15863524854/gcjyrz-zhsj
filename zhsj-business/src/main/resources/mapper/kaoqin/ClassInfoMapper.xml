<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhsj.business.kaoqin.mapper.ClassInfoMapper">
    <resultMap id="classInfoResult" type="com.zhsj.business.kaoqin.dto.ClassInfoDto">
        <result column="class_code" property="classCode"/>
        <result column="class_name" property="className"/>
    </resultMap>
    <select id="listClassInfo" resultMap="classInfoResult">
        SELECT class_code,class_name FROM `business_class`
    </select>
</mapper>
