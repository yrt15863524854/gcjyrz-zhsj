<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhsj.business.test.mapper.TestMapper">
    <select id="getAllTest" resultType="Test">
        select * from test
    </select>
    <select id="dividePaper" parameterType="com.zhsj.business.test.domain.PaperParameter" resultType="Test">
        select * from test
        limit #{dto.pageNum},#{dto.pageSize}
    </select>
</mapper>
